<%- include('partials/header') %>
<h2>All Posts</h2>
<ul>
    <% posts.forEach((post) => { %>
        <li>
            <a href="/posts/<%= post.idx %>" class="link-underline link-underline-opacity-0 link-underline-opacity-75-hover"><%= post.title %></a>
        </li>
    <% }) %>
</ul>
<%- include('partials/footer') %>